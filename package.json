{
  "name": "bot-crawl",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^24.5.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "mezon-sdk": "^2.8.13",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "puppeteer": "^24.20.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.26"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/pg": "^8.15.5",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "typeorm": "tsx ./node_modules/typeorm/cli.js",
    "migration:generate": "yarn typeorm migration:generate ./src/migrations/InitSchema -d ./src/data-source.ts",
    "migration:run": "yarn typeorm migration:run -d ./src/data-source.ts",
    "migration:revert": "yarn typeorm migration:revert -d ./src/data-source.ts"
  }
}
